<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userinfo">
        <insert id="saveUser" parameterType="java.util.HashMap" >
        	<![CDATA[
            	insert into USER (user_name,user_age,user_sex,inputtime)
			        	values(#{name},#{age},#{sex},#{inputtime})
			]]>
        </insert>
        
        <delete id="deleteUser" parameterType="java.lang.String" >
            delete from USER where id = #{id};
        </delete>
        
        <select id="searchUser" parameterType="java.util.HashMap" resultType="java.util.HashMap">
        	<![CDATA[
        		select * from USER t
        	]]>
        </select>
</mapper>
